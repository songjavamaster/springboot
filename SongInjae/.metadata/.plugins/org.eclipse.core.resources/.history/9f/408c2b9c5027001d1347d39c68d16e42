package com.myspring;



@Controller
public class DemoController {
	@ResponseBody
	@RequestMapping("/")
	public String home() {
		System.out.println("Hello Boot!!");
		return "Hello Boot!!";
	}
	@RequestMapping("/SongInJae.do")
	   public String hello(Model model) {
	      System.out.println("안녕하세요");
	      model.addAttribute("message", "hello.jsp입니다.!");
	      return "hello";
	   }
	
}
